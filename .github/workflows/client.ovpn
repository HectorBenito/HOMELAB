client
dev tun
proto udp
float
nobind
data-ciphers-fallback AES-256-CBC
comp-lzo no
resolv-retry infinite
remote-cert-tls server
persist-key
auth-user-pass
explicit-exit-notify
remote ${{ secrets.OVPN_SERVER_HOST }} ${{ secrets.OVPN_SERVER_PORT }}
${{ secrets.OVPN_SERVER_CA }}
${{ secrets.OVPN_SERVER_CERT }}

